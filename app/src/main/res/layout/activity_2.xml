<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.tnovacic.novathon.Activity2">

    <LinearLayout
        android:id="@+id/food_bar"
        android:layout_width="65dp"
        android:layout_height="30dp"
        android:layout_marginTop="16dp"
        android:orientation="horizontal"
        app:layout_constraintRight_toLeftOf="@+id/drinks_bar"
        app:layout_constraintTop_toTopOf="parent"></LinearLayout>

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/floatingActionButton"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:clickable="true"
        app:fabSize="mini"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.898"
        app:srcCompat="@drawable/btn_random_scan_clear"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp" />

    <TextView
        android:id="@+id/food"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:layout_marginLeft="123dp"
        android:layout_marginStart="123dp"
        android:layout_marginTop="80dp"
        android:text="Food"
        android:textSize="18sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/bills"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:layout_marginLeft="129dp"
        android:layout_marginStart="129dp"
        android:layout_marginTop="10dp"
        android:text="Bills"
        android:textSize="18sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/drinks" />

    <TextView
        android:id="@+id/Fuel"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:layout_marginLeft="129dp"
        android:layout_marginStart="129dp"
        android:layout_marginTop="21dp"
        android:text="Fuel"
        android:textSize="18sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bills" />

    <TextView
        android:id="@+id/Other"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:layout_marginLeft="57dp"
        android:layout_marginStart="57dp"
        android:layout_marginTop="16dp"
        android:text="  Other"
        android:textSize="18sp"
        app:layout_constraintLeft_toRightOf="@+id/linearLayout5"
        app:layout_constraintTop_toBottomOf="@+id/Fuel" />

    <TextView
        android:id="@+id/total"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:layout_marginLeft="111dp"
        android:layout_marginStart="111dp"
        android:layout_marginTop="20dp"
        android:text="TOTAL"
        android:textSize="20sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Other" />

    <TextView
        android:id="@+id/Remaining"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:layout_marginLeft="91dp"
        android:layout_marginStart="91dp"
        android:layout_marginTop="15dp"
        android:text="Remaining"
        android:textSize="18sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/total" />

    <TextView
        android:id="@+id/food_num"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="81dp"
        android:layout_marginRight="81dp"
        android:layout_marginTop="80dp"
        android:text="0,00"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/drink_num"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="81dp"
        android:layout_marginRight="81dp"
        android:layout_marginTop="15dp"
        android:text="0,00"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/food_num" />

    <TextView
        android:id="@+id/bills_num"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="81dp"
        android:layout_marginRight="81dp"
        android:layout_marginTop="11dp"
        android:text="0,00"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/drink_num" />

    <TextView
        android:id="@+id/fuel_num"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="81dp"
        android:layout_marginRight="81dp"
        android:layout_marginTop="19dp"
        android:text="0,00"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bills_num" />

    <TextView
        android:id="@+id/other_num"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="81dp"
        android:layout_marginRight="81dp"
        android:layout_marginTop="15dp"
        android:text="0,00"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fuel_num" />

    <TextView
        android:id="@+id/total_num"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="80dp"
        android:layout_marginRight="80dp"
        android:layout_marginTop="18dp"
        android:text="0,00"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/other_num" />

    <TextView
        android:id="@+id/rem_num"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="81dp"
        android:layout_marginRight="81dp"
        android:layout_marginTop="14dp"
        android:text="0,00"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/total_num" />

    <TextView
        android:id="@+id/drinks"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:layout_marginLeft="116dp"
        android:layout_marginStart="116dp"
        android:layout_marginTop="19dp"
        android:text="Drinks"
        android:textSize="18sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/food" />

    <LinearLayout
        android:id="@+id/drinks_bar"
        android:layout_width="65dp"
        android:layout_height="30dp"
        android:orientation="horizontal"
        app:layout_constraintRight_toLeftOf="@+id/bills_bar"
        android:layout_marginRight="1dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="1dp"></LinearLayout>

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="click"
        android:text="Button"
        tools:layout_editor_absoluteX="263dp"
        tools:layout_editor_absoluteY="368dp" />

    <LinearLayout
        android:id="@+id/bills_bar"
        android:layout_width="65dp"
        android:layout_height="30dp"
        android:orientation="horizontal"
        app:layout_constraintRight_toLeftOf="@+id/fuel_bar"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="16dp">

    </LinearLayout>

    <LinearLayout
        android:id="@+id/fuel_bar"
        android:layout_width="65dp"
        android:layout_height="30dp"
        android:orientation="horizontal"
        app:layout_constraintRight_toLeftOf="@+id/other_bar"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="16dp"></LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="23dp"
        android:layout_height="25dp"
        android:layout_marginLeft="31dp"
        android:layout_marginTop="80dp"
        android:background="#3F51B5"
        android:orientation="horizontal"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginStart="31dp"></LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="23dp"
        android:layout_height="25dp"
        android:layout_marginLeft="31dp"
        android:layout_marginTop="12dp"
        android:background="#64B5F6"
        android:orientation="horizontal"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout"
        android:layout_marginStart="31dp"></LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="23dp"
        android:layout_height="25dp"
        android:layout_marginLeft="31dp"
        android:layout_marginTop="7dp"
        android:background="#F4511E"
        android:orientation="horizontal"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout2"
        android:layout_marginStart="31dp"></LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout4"
        android:layout_width="23dp"
        android:layout_height="25dp"
        android:layout_marginLeft="31dp"
        android:layout_marginTop="16dp"
        android:background="#CDDC39"
        android:orientation="horizontal"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout3"
        android:layout_marginStart="31dp"></LinearLayout>

    <LinearLayout
        android:layout_width="23dp"
        android:layout_height="25dp"
        android:layout_marginLeft="31dp"
        android:layout_marginTop="11dp"
        android:background="#8D6E63"
        android:orientation="horizontal"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout4"
        android:id="@+id/linearLayout5"
        android:layout_marginStart="31dp"></LinearLayout>

    <LinearLayout
        android:id="@+id/koliko"
        android:layout_width="326dp"
        android:layout_height="10dp"
        android:orientation="horizontal"
        tools:layout_editor_absoluteX="25dp"
        tools:layout_editor_absoluteY="3dp"></LinearLayout>

    <LinearLayout
        android:id="@+id/other_bar"
        android:layout_width="65dp"
        android:layout_height="30dp"
        android:layout_marginEnd="33dp"
        android:layout_marginRight="33dp"
        android:layout_marginTop="16dp"
        android:orientation="horizontal"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"></LinearLayout>

</android.support.constraint.ConstraintLayout>
